function solution(str){
    let stringArray = str.split("")
    let reversedStringArray = []
    
    for (let k = 0; k<str.length;k++){
        reversedStringArray.push(stringArray.pop())
    }
    
    
    let strRev = reversedStringArray.join()
    strRev = strRev.replaceAll(",","")
    
    console.log(str)
    console.log(strRev)
}